{
	"version": "2.0.0",
	"type": "shell",
	"presentation": {
		"echo": false,
		"reveal": "always",
		"panel": "dedicated",
		"showReuseMessage": false,
		"clear": true,
		"revealProblems": "onProblem",
		"close": true
	},
	"tasks": [
		{
			"label": "Make",
			"command": "make > make.log",
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "Test",
			"command": "make test.md > test.md",
			"group": {
				"kind": "build",
				"isDefault": false
			}
		}
	]
}
